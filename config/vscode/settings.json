  "vim.normalModeKeyBindingsNonRecursive": [
    // NAVIGATION
    // switch b/w buffers
    {
      "before": [
        "<S-h>"
      ],
      "commands": [
        ":bprevious",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "<S-l>"
      ],
      "commands": [
        ":bnext",
        "extension.vim_escape"
      ]
    },
    // splits
    {
      "before": [
        "leader",
        "v"
      ],
      "commands": [
        "workbench.action.newGroupRight"
      ]
    },
    {
      "before": [
        "leader",
        "s"
      ],
      "commands": [
        "workbench.action.newGroupBelow"
      ]
    },
    // panes
    {
      "before": [
        "leader",
        "h"
      ],
      "commands": [
        "workbench.action.focusLeftGroup",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "leader",
        "j"
      ],
      "commands": [
        "workbench.action.focusBelowGroup",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "leader",
        "k"
      ],
      "commands": [
        "workbench.action.focusAboveGroup",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "leader",
        "l"
      ],
      "commands": [
        "workbench.action.focusRightGroup",
        "extension.vim_escape"
      ]
    },
    // NICE TO HAVE
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        ":wa!"
      ]
    },
    {
      "before": [
        "leader",
        "q"
      ],
      "commands": [
        ":q!"
      ]
    },
    {
      "before": [
        "leader",
        "x"
      ],
      "commands": [
        ":x!"
      ]
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.marker.prev"
      ]
    },
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "leader",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "leader",
        "p"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "g",
        "h"
      ],
      "commands": [
        "editor.action.showDefinitionPreviewHover"
      ]
    },
    // hack command vim
    {
      "before": [
        "p"
      ],
      "commands": [
        "editor.action.clipboardPasteAction",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "d"
      ],
      "after": [
        "\"",
        "_",
        "d"
      ]
    },
    {
      "before": [
        "D"
      ],
      "after": [
        "\"",
        "_",
        "D"
      ]
    },
    {
      "before": [
        "d",
        "d"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "d"
      ]
    },
    {
      "before": [
        "c"
      ],
      "after": [
        "\"",
        "_",
        "c"
      ]
    },
    {
      "before": [
        "C"
      ],
      "after": [
        "\"",
        "_",
        "C"
      ]
    },
    {
      "before": [
        "x"
      ],
      "after": [
        "\"",
        "_",
        "x"
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    // set move view center of row
    {
      "before": [
        "j"
      ],
      "after": [
        "j",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "k",
        "z",
        "z"
      ]
    },
    // set fast move 5 row
    // { "before": ["J"], "after": ["5", "j", "z", "z"] },
    // { "before": ["K"], "after": ["5", "k", "z", "z"] }
    {
      "before": [
        "J"
      ],
      "after": [
        "<C-d>",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "<C-u>",
        "z",
        "z"
      ]
    },
    // set insert line break
    // {
    //   "before": ["<S-n>"],
    //   "commands": ["lineBreakInsert"],
    //   "silent": true
    // }
    {
      "before": [
        "z",
        "j"
      ],
      "after": [
        "o",
        "<Esc>"
      ]
    },
    {
      "before": [
        "z",
        "k"
      ],
      "after": [
        "O",
        "<Esc>"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>",
        "l"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    // Stay in visual mode while indenting
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    // Move selected lines while staying in visual mode
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.moveLinesUpAction"
      ]
    },
    {
      "before": [
        "J"
      ],
      "commands": [
        "editor.action.moveLinesDownAction"
      ]
    },
    // toggle comment selection
    {
      "before": [
        "leader",
        "c"
      ],
      "commands": [
        "editor.action.commentLine",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "X"
      ],
      "commands": [
        "editor.action.clipboardCutAction",
        "extension.vim_escape"
      ]
    },
    {
      "before": [
        "y"
      ],
      "commands": [
        "editor.action.clipboardCopyAction",
        "extension.vim_escape"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "x"
      ],
      "after": [
        "\"",
        "_",
        "x"
      ]
    }
  ],
